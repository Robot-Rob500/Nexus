  <Code><![CDATA[
$formShares_Load = {
	
	$formShares.StartPosition = 'Manual'
	$formShares.Location = $formAnchorShares.PointToScreen($formShares.Location)
	
	$sharesArray = @(); $sharesArray = get-WmiObject -class Win32_Share -computer $($computername_field.Text)
	$listViewObjectArray = @()
	$index = 0
	foreach ($object in $sharesArray)
	{
		Add-ListViewItem -ListView $shareList -Items "" -ImageIndex 0 -SubItems $object.Name, $object.Path, $object.Description
		$shareList.Items[$index].ToolTipText = $($object.Path)
		$index++
	}
	Sort-ListViewColumn -ListView $shareList -ColumnIndex 1 -SortOrder 'Ascending'
	$formShares.Add_Shown({ $formShares.Activate(); $formShares.focus() })
	
	#region Window Foreground
	Mainform-Foreground
	#endregion
}

#region Functions

$shareList_SelectedIndexChanged = {
	$script:share = $null
	$ErrorActionPreference = 'SilentlyContinue'
	$script:share = $shareList.SelectedItems[0].SubItems[1].Text
	$script:shareIndex = $shareList.SelectedIndices[0]
}

$shareList_ItemActivate = {
	
	if ($script:share -notlike "*\\*")
	{
		$sharePath = "\\" + "$($computername_field.Text)" + "\" + "$($shareList.Items[$shareIndex].SubItems[1].Text)"
		if ((Test-Path -path "$sharePath") -eq $true)
		{
			explorer.exe $sharePath
		}
		else
		{
			$global:errorMessage = "Unable to connect. This may be due to insufficient permissions."
			Call-error_psf
		}
	}
	else
	{
		$sharePath = $($shareList.Items[$shareIndex].SubItems[1].Text)
		if ((Test-Path -path "$sharePath") -eq $true)
		{
			explorer.exe $sharePath
		}
		else
		{
			$global:errorMessage = "Unable to connect. This may be due to insufficient permissions."
			Call-error_psf
		}
	}
	
}

$buttonSharesClose_Click={ $formShares.Close() }

$windowBarPanel_MouseDown = [System.Windows.Forms.MouseEventHandler]{
	
	$cursorPosition = [System.Windows.Forms.Cursor]::Position
	$controlPosition = $windowBarPanel.PointToClient((New-Object System.Drawing.Point($_.X, $_.Y)))
	$script:diffX = ($cursorPosition.X + $controlPosition.X)
	$script:diffy = ($cursorPosition.Y + $controlPosition.Y)
	$windowMoveTimer.Start()
	
}

$windowBarPanel_MouseUp = [System.Windows.Forms.MouseEventHandler]{
	
	$script:diffx = 0
	$script:diffy = 0
	$windowMoveTimer.Stop()
	
}

$windowMoveTimer_Tick = {
	
	$cursorPosition = [System.Windows.Forms.Cursor]::Position
	$newPosX = $cursorPosition.X - ($diffx/2)
	if (($cursorPosition.Y - ($diffy/2)) -lt 0)
	{ $newPosY = 0 }
	Else
	{ $newPosY = $cursorPosition.Y - ($diffy/2) }
	$formShares.SetDesktopLocation($newPosX, $newPosY)
	
}

$copySharePathToolStripMenuItem_Click={ Set-Clipboard -Value "$script:share" }

$driveSpaceToolStripMenuItem_Click={
	
	$spaceSniffer = "$toolsPath\SpaceSniffer.exe"
	$sharePath = "\\" + "$($computername_field.Text)" + "\" + "$($shareList.Items[$shareIndex].SubItems[1].Text)"
	$spaceArgs = "scan $sharePath"
	Start $spaceSniffer -ArgumentList "$spaceArgs"
	
}
]]></Code>
