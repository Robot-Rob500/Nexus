  <Code><![CDATA[
$formFolders_Load = {
	
	$formFolders.StartPosition = 'Manual'
	$formFolders.Location = $formSitesAnchor.PointToScreen($formFolders.Location)
	$newLoc = $formFolders.Location; $newLoc.X = $newLoc.X - 304
	$formFolders.Location = $newLoc
	
	$folderArray = @(); $folderArray = Get-ChildItem -Path "HKCU:\Software\Nexus\Folders" -Recurse
	$listViewObjectArray = @()
	$index = 0
	foreach ($object in $folderArray)
	{
		$propertyPath = $object.Name.Replace("HKEY_CURRENT_USER", "HKCU:")
		$folderProperties = Get-ItemProperty -Path "$propertyPath"

		Add-ListViewItem -ListView $folderList -Items "" -ImageIndex 0 -SubItems $folderProperties.Description, $folderProperties.Path, $propertyPath
		$folderList.Items[$index].ToolTipText = $($folderProperties.Path)
		$index++
	}
	Sort-ListViewColumn -ListView $folderList -ColumnIndex 1 -SortOrder 'Ascending'
	$formFolders.Add_Shown({ $formFolders.Activate(); $formFolders.focus() })
	
	#region Variables
	$global:folderEditData = @()
	#endregion
}

#region Functions

$folderList_SelectedIndexChanged = {
	$folder = $null
	$ErrorActionPreference = 'SilentlyContinue'
	$script:folder = $folderList.SelectedItems[0].SubItems[2].Text
	$script:folder = $script:site.Split("(")[0]
	$script:folderIndex = $folderList.SelectedIndices[0]
}

$folderList_ItemActivate = { explorer.exe $($folderList.Items[$folderIndex].SubItems[2].Text) }

$removeSelectedFolderToolStripMenuItem_Click={
	
	$regPath = $folderList.Items[$folderIndex].SubItems[3].Text
	Write-Host $regPath
	Remove-Item -Path "$regPath" -Recurse
	$folderList.Items[$folderIndex].Remove()
	$folderList.Refresh()
	
}

$folderOptions_Opened={ $folderOptions.Focus() }

$buttonFoldersClose_Click={ $formFolders.Close() }

$buttonAddFolder_Click = { Call-folder-add_psf; Reopen-Folders }

$addFolderToolStripMenuItem_Click = { Call-folder-add_psf; $folderList.Items.Clear(); Reopen-Folders }

$copyUNCPathToolStripMenuItem_Click = { Set-Clipboard -Value "$script:folder"; write-Host $script:folder }

$exportFoldersToolStripMenuItem_Click={
	
	$folderData = Get-ChildItem -Path "HKCU:\Software\Nexus\Folders" | Get-ItemProperty
	$count = $folderData.Count
	$folderArray = @()
	for ($index = 0; $index -lt $count; $index++)
	{
		$folderObj = New-Object PSObject -Property @{
			
			Description = $folderData[$index].Description
			Path = $folderData[$index].Path
			PSPath = $folderData[$index].PSPath.Split("\")[-1]
		}
		$folderArray += $folderObj
	}
	$saveFileDialog = New-Object System.Windows.Forms.SaveFileDialog
	$saveFileDialog.initialDirectory = $initialDirectory
	$saveFileDialog.filter = "CSV (*.csv)| *.csv"
	$saveFileDialog.ShowDialog() | Out-Null
	
	$saveLocation = $saveFileDialog.FileName
	$folderArray | Select Description, Path, PSPath | Export-CSV -Path "$saveLocation" -NoTypeInformation
	
}

$importFoldersToolStripMenuItem_Click={
	
	$OpenFileDialog = New-Object System.Windows.Forms.OpenFileDialog
	$OpenFileDialog.initialDirectory = $initialDirectory
	$OpenFileDialog.filter = "CSV (*.csv)| *.csv"
	$OpenFileDialog.ShowDialog() | Out-Null
	
	$folders = Import-CSV -Path $OpenFileDialog.Filename
	
	foreach ($folder in $folders)
	{
		$Path = "HKCU:\Software\Nexus\Folders\" + $folder.PSPath
		New-Item "$Path" -Force
		New-ItemProperty -Path "$Path" -Name Description -PropertyType String -Value $folder.Description -Force
		New-ItemProperty -Path "$Path" -Name Path -PropertyType String -Value $folder.Path -Force
	}
	Reopen-Folders
}

$windowBarPanel_MouseDown = [System.Windows.Forms.MouseEventHandler]{
	
	$cursorPosition = [System.Windows.Forms.Cursor]::Position
	$controlPosition = $windowBarPanel.PointToClient((New-Object System.Drawing.Point($_.X, $_.Y)))
	$script:diffX = ($cursorPosition.X + $controlPosition.X)
	$script:diffy = ($cursorPosition.Y + $controlPosition.Y)
	$windowMoveTimer.Start()
	
}

$windowBarPanel_MouseUp = [System.Windows.Forms.MouseEventHandler]{
	
	$script:diffx = 0
	$script:diffy = 0
	$windowMoveTimer.Stop()
	
}

$windowMoveTimer_Tick = {
	
	$cursorPosition = [System.Windows.Forms.Cursor]::Position
	$newPosX = $cursorPosition.X - ($diffx/2)
	if (($cursorPosition.Y - ($diffy/2)) -lt 0)
	{ $newPosY = 0 }
	Else
	{ $newPosY = $cursorPosition.Y - ($diffy/2) }
	$formFolders.SetDesktopLocation($newPosX, $newPosY)
	
}

$editFolderToolStripMenuItem_Click = {
	
	$global:folderEditData = New-Object PSObject -Property @{
		
	Description = $($folderList.Items[$folderIndex].SubItems[1].Text)
	Path = $($folderList.Items[$folderIndex].SubItems[2].Text)
		
	}
	
	Call-folder-add_psf
	Reopen-Folders
}
]]></Code>
