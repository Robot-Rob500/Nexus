	})
	$((Get-Variable -Name "panel3Button$index").value).Add_MouseEnter({ $global:focusedControl = $panel3ObjectArray[$this.Tag].ButtonName; $global:focusedControlIndex = $this.Tag; $global:selectedPanel = 3 })
