  <Code><![CDATA[
$formDropdown_Load = {
	
	$formDropdown.StartPosition = 'Manual'
	$dropdownCoords = $MainForm.PointToScreen($formDropdown.Location)
	$dropdownCoords.Y = $dropdownCoords.Y + $MainForm.Height
	$formDropdown.MaximumSize = "$($MainForm.Width),1000"
	$formDropdown.Width = $($MainForm.Width)
	$formDropdown.Location = $dropdownCoords
	$formDropdown.Opacity = 0
	$fadeInTimer.Start()
	
	#region Variables
	$global:editButtonData = @()
	
	$panel1layoutpanel.AllowDrop = $true
	
	#region Dropdown Label Positioning
	if ($panel1layoutpanel.Height -gt $panel2layoutpanel.Height)
	{
		[int]$script:labelLocation = $panel1layoutpanel.Height + $flowControlPanel.Location.Y
	}
	else
	{
		[int]$script:labelLocation = $panel2layoutpanel.Height + $flowControlPanel.Location.Y
	}
	
	$panel2.Location = "0,$script:labelLocation"
	#endregion
	
	#region Dropdown Label Display
	$script:displayLabel2 = (Get-ItemProperty -Path "HKCU:\Software\Nexus\DropdownButtons\" -Name "DisplayLabel2").DisplayLabel2
	$script:displayRow2 = (Get-ItemProperty -Path "HKCU:\Software\Nexus\DropdownButtons\" -Name "DisplayRow2").DisplayRow2
	
	if ($script:displayLabel2 -notlike 'Checked') { $panel2.Visible = $false }
	if ($script:displayRow2 -notlike 'Checked') { $panel3layoutpanel.Visible = $false; $panel4layoutpanel.Visible = $false }
	#endregion
	
	#region Dropdown Foreground
	Mainform-Foreground
	#endregion
}

$formDropdown_MouseLeave = { $dropdownCloseTimer.Start() }

$formDropdown_MouseEnter = { $dropdownCloseTimer.Stop() }

$dropdownCloseTimer_Tick = {
	
	$cursorPosition = [System.Windows.Forms.Cursor]::Position
	$cursorPositionX = $cursorPosition.X
	$cursorPositionY = $cursorPosition.Y
	
	$windowPosition = $formDropdown.Location
	$windowPositionX1 = $windowPosition.X
	$windowPositionY1 = $windowPosition.Y
	
	$windowPositionX2 = $windowPositionX1 + $formDropdown.Size.Width
	$windowPositionY2 = $windowPositionY1 + $formDropdown.Size.Height
	
	if (($cursorPositionX -gt $windowPositionX2) -or ($cursorPositionX -lt $windowPositionX1) -or ($cursorPositionY -lt $windowPositionY1) -or ($cursorPositionY -gt $windowPositionY2))
	{ $formDropdown.Close() }
}

#region Panel1 Layout Panel

$panel1Label.Text = (Get-ItemProperty -path "HKCU:\Software\Nexus\DropdownButtons\Panel1").Label
$panel1Array = (get-childitem -path "HKCU:\Software\Nexus\DropdownButtons\Panel1").PSObject.BaseObject | Get-ItemProperty

$buttonName = $null
$iconPath = $null
$programPath = $null
$arguments = $null
$count = 0
$panel1ObjectArray = @()

foreach ($object in $panel1Array)
{
	$panel1Object = New-Object PSObject -Property @{
		
		arguments = $object.arguments
		ButtonName = $object.ButtonName
		Iconpath = $object.IconPath
		programPath = $object.programPath
		
	}
	$panel1ObjectArray += $panel1Object
	$count++
}

for ($index = 0; $index -lt $count; $index++)
{
	
	$buttonName = $panel1ObjectArray[$index].ButtonName
	$iconPath = $panel1ObjectArray[$index].IconPath
	$programPath = $panel1ObjectArray[$index].programPath
	$arguments = $panel1ObjectArray[$index].arguments
	
	if (($iconPath -notlike $null) -and ($iconPath -notlike "")) { $buttonImage = [System.Drawing.Image]::FromFile($iconPath) }
	$newButton = New-Object System.Windows.Forms.Button
	$newButton.Size = New-Object System.Drawing.Size(73, 65)
	if (($iconPath -notlike $null) -and ($iconPath -notlike "")) { $newButton.Image = $buttonImage }
	$newButton.TextAlign = 'MiddleCenter'
	$newButton.ImageAlign = 'TopCenter'
	$newButton.TextImageRelation = 'ImageAboveText'
	$newButton.ForeColor = 'White'
	$newButton.FlatStyle = 'Flat'
	$newButton.Margin = '0, 0, 0, 0'
	$newButton.Padding = '0, 0, 0, 0'
	$newButton.Flatappearance.BorderSize = '0'
	$newButton.Flatappearance.MouseOverBackColor = 'DimGray'
	$newButton.Text = $buttonName
	$newButton.Tag = $index
	New-Variable -Name "panel1Button$index" -Value $newButton
}
for ($index = 0; $index -lt $count; $index++)
{
	$((Get-Variable -Name "panel1Button$index").value).Add_Click({ Invoke-Command -ScriptBlock { if ($panel1ObjectArray[$this.Tag].arguments[0] -like "") { start $panel1ObjectArray[$this.Tag].programPath } }
		else { start $panel1ObjectArray[$this.Tag].programPath $panel1ObjectArray[$this.Tag].arguments; $formDropdown.Close() } })
	$((Get-Variable -Name "panel1Button$index").value).Add_MouseEnter({ $global:focusedControl = $panel1ObjectArray[$this.Tag].ButtonName; $global:focusedControlIndex = $this.Tag; $global:selectedPanel = 1 })
	$tooltip.SetToolTip($((Get-Variable -Name "panel1Button$index").value), "$($panel1ObjectArray[$((Get-Variable -Name "panel1Button$index").value).Tag].programPath) $($panel1ObjectArray[$((Get-Variable -Name "panel1Button$index").value).Tag].arguments)")
	$panel1LayoutPanel.Controls.Add($((Get-Variable -Name "panel1Button$index").value))
}

#endregion

#region Panel2 Layout Panel

$panel2Label.Text = (Get-ItemProperty -path "HKCU:\Software\Nexus\DropdownButtons\Panel2").Label
$panel2Array = (get-childitem -path "HKCU:\Software\Nexus\DropdownButtons\Panel2").PSObject.BaseObject | Get-ItemProperty

$buttonName = $null
$iconPath = $null
$programPath = $null
$arguments = $null
$count = 0
$panel2ObjectArray = @()

foreach ($object in $panel2Array)
{
	$panel2Object = New-Object PSObject -Property @{
		
		arguments = $object.arguments
		ButtonName = $object.ButtonName
		Iconpath = $object.IconPath
		programPath = $object.programPath
		
	}
	$panel2ObjectArray += $panel2Object
	$count++
}

for ($index = 0; $index -lt $count; $index++)
{
	
	$buttonName = $panel2ObjectArray[$index].ButtonName
	$iconPath = $panel2ObjectArray[$index].IconPath
	$programPath = $panel2ObjectArray[$index].programPath
	$arguments = $panel2ObjectArray[$index].arguments
	
	if (($iconPath -notlike $null) -and ($iconPath -notlike "")) { $buttonImage = [System.Drawing.Image]::FromFile($iconPath) }
	$newButton = New-Object System.Windows.Forms.Button
	$newButton.Size = New-Object System.Drawing.Size(73, 65)
	if (($iconPath -notlike $null) -and ($iconPath -notlike "")) { $newButton.Image = $buttonImage }
	$newButton.TextAlign = 'MiddleCenter'
	$newButton.ImageAlign = 'TopCenter'
	$newButton.TextImageRelation = 'ImageAboveText'
	$newButton.ForeColor = 'White'
	$newButton.FlatStyle = 'Flat'
	$newButton.Margin = '0, 0, 0, 0'
	$newButton.Padding = '0, 0, 0, 0'
	$newButton.Flatappearance.BorderSize = '0'
	$newButton.Flatappearance.MouseOverBackColor = 'DimGray'
	$newButton.Text = $buttonName
	$newButton.Tag = $index
	New-Variable -Name "panel2Button$index" -Value $newButton
}
for ($index = 0; $index -lt $count; $index++)
{
	$((Get-Variable -Name "panel2Button$index").value).Add_Click({ Invoke-Command -ScriptBlock { if ($panel2ObjectArray[$this.Tag].arguments[0] -like "") { start $panel2ObjectArray[$this.Tag].programPath }
			else { start $panel2ObjectArray[$this.Tag].programPath $panel2ObjectArray[$this.Tag].arguments }; $formDropdown.Close()
		}
	})
	$((Get-Variable -Name "panel2Button$index").value).Add_MouseEnter({ $global:focusedControl = $panel2ObjectArray[$this.Tag].ButtonName; $global:focusedControlIndex = $this.Tag; $global:selectedPanel = 2 })
