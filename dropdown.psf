	elseif ((($variableName -eq "panel1LayoutPanel") -or ($variableName -eq "panel2LayoutPanel")) -and ($((Get-Variable -Name "panel$($global:selectedPanel)LayoutPanel").value).Controls.Count -eq 100))
	{
		$addButtonToolStripMenuItem.Enabled = $false
