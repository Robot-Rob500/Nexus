  <Object type="System.Windows.Forms.ToolTip, System.Windows.Forms, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" name="tooltipButtons" />
  <Code><![CDATA[
$formSites_Load = {

	$formSites.StartPosition = 'Manual'
	$formSites.Location = $formSitesAnchor.PointToScreen($formSites.Location)
	$newLoc = $formSites.Location; $newLoc.X = $newLoc.X - 394
	$formSites.Location = $newLoc
	
	$groupArray = @(); $groupArray = Get-Item -Path "HKCU:\Software\Nexus\WebsiteGroups"
	$siteList.Groups.AddRange($groupArray.Property)
	
	$browserArray = @(); $browserArray = Get-ChildItem -Path "HKCU:\Software\Nexus\WebBrowsers" -Recurse
	$imageIndexArray = @()
	$script:browserPathArray = @()
	$index = 0
	
	foreach ($object in $browserArray)
	{
		$objectPath = $null; $objectPath = $object.Name.Replace("HKEY_CURRENT_USER", "HKCU:")
		$iconPath = Get-ItemPropertyValue -Path $objectPath -Name IconPath
		$browserPath = Get-ItemPropertyValue -Path $objectPath -Name ProgramPath
		$icon = [System.Drawing.Bitmap]::FromFile($iconPath)
		$browserIconList.Images.Add($icon)
		
		$imageIndexObj = New-Object PSObject -Property @{
			
			Index = $index
			ImageName = $objectPath.Split("\")[-1].Replace(".ico",'')
			
		}
		
		$browserArrayObj = New-Object PSObject -Property @{
			
			Browser = $objectPath.Split("\")[-1]
			Path = $browserPath
		}
		
		$script:browserPathArray += $browserArrayObj
		$imageIndexArray += $imageIndexObj
		$index++
	}
	
	$siteArray = @(); $siteArray = Get-ChildItem -Path "HKCU:\Software\Nexus\Websites" -Recurse
	$listViewObjectArray = @()
	$index = 0
	$siteArrayTable = @()
	
	foreach ($object in $siteArray)
	{
		$propertyPath = $object.Name.Replace("HKEY_CURRENT_USER", "HKCU:")
		$siteProperties = Get-ItemProperty -Path "$propertyPath"
		$siteArrayTable += $siteProperties
	}
	
	$siteArrayTable = $siteArrayTable | Sort-Object Groupname
	
	foreach ($object in $siteArrayTable)
	{
		$propertyPath = $object.PSPath.Replace("Microsoft.PowerShell.Core\Registry::HKEY_CURRENT_USER", "HKCU:")
		foreach ($item in $imageIndexArray)
		{
			if ($object.Browser -like $item.ImageName)
			{
				$imageIndex = $item.Index
			}
			
		}
		Add-ListViewItem -ListView $siteList -Items "" -Group $object.Groupname -ImageIndex $imageIndex -SubItems $object.Description, $object.URL, $object.AccountType, $object.Groupname, $propertyPath, $object.WebProtocol, $object.Browser
		$siteList.Items[$index].ToolTipText = $($object.URL)
		$index++
	}
	Sort-ListViewColumn -ListView $siteList -ColumnIndex 1 -SortOrder 'Ascending'
	$formSites.Add_Shown({ $formSites.Activate(); $formSites.focus() })
	
	#region Variables
	$global:sitesEditData = @()
	#endregion
	
	#region Window Foreground
	Mainform-Foreground
	#endregion
}

$windowBarPanel_MouseDown = [System.Windows.Forms.MouseEventHandler]{
	
	$cursorPosition = [System.Windows.Forms.Cursor]::Position
	$controlPosition = $windowBarPanel.PointToClient((New-Object System.Drawing.Point($_.X, $_.Y)))
	$script:diffX = ($cursorPosition.X + $controlPosition.X)
	$script:diffy = ($cursorPosition.Y + $controlPosition.Y)
	$windowMoveTimer.Start()
	
}

$windowBarPanel_MouseUp = [System.Windows.Forms.MouseEventHandler]{
	
	$script:diffx = 0
	$script:diffy = 0
	$windowMoveTimer.Stop()
	
}

$windowMoveTimer_Tick = {
	
	$cursorPosition = [System.Windows.Forms.Cursor]::Position
	$newPosX = $cursorPosition.X - ($diffx/2)
	if (($cursorPosition.Y - ($diffy/2)) -lt 0)
	{ $newPosY = 0 }
	Else
	{ $newPosY = $cursorPosition.Y - ($diffy/2) }
	$formSites.SetDesktopLocation($newPosX, $newPosY)
	
}

$siteList_SelectedIndexChanged = {
	$site = $null
	$ErrorActionPreference = 'SilentlyContinue'
	$script:site = $siteList.SelectedItems[0].SubItems[2].Text
	$script:site = $script:site.Split("(")[0]
	$script:siteIndex = $siteList.SelectedIndices[0]
	$accountLevel = $siteList.SelectedItems[0].SubItems[3].Text

	if ($accountLevel -like 'No Preference') { $global:cred = $null }
	if ($accountLevel -like 'Standard') { $global:cred = New-Object System.Management.Automation.PSCredential -ArgumentList @($usercred_user, (ConvertTo-SecureString -String $usercred_pass -AsPlainText -Force)); }
	if ($accountLevel -like 'Administrator') { $global:cred = New-Object System.Management.Automation.PSCredential -ArgumentList @($admincred_user, (ConvertTo-SecureString -String $admincred_pass -AsPlainText -Force)); }
	
}

$siteList_ItemActivate = {
	
	If ($site -ne $null)
	{
		$newBrowser = $null
		foreach ($object in $script:browserPathArray)
		{
			if ($object.Browser -like $siteList.Items[$siteIndex].SubItems[7].Text)
			{
				$newBrowser = $object.Path
			}
		}
		
		if ($newBrowser -like "*MicrosoftEdge.exe")
		{
			if ($site -notlike "http*")
			{
				$site = "http://$site"
				start microsoft-edge:$site
			}
			Else
			{
				start microsoft-edge:$site
			}
		}
		Elseif ($newBrowser -eq $null)
		{
			start $site
		}
		Else
		{
			if (($siteList.Items[$siteIndex].SubItems[3].Text -like "Standard") -or ($siteList.Items[$siteIndex].SubItems[3].Text -like "Administrator"))
			{
				Start-Process $newBrowser -ArgumentList $site -LoadUserProfile -Credential $cred
			}
			elseif (($siteList.Items[$siteIndex].SubItems[3].Text -like "No Preference") -or ($siteList.Items[$siteIndex].SubItems[3].Text -like ""))
			{
				Start-Process $newBrowser -ArgumentList $site
			}
		}
	}
	
}

$removeSelectedSiteToolStripMenuItem_Click={
	
	$regPath = $siteList.Items[$siteIndex].SubItems[5].Text
	Remove-Item -Path "$regPath" -Recurse
	$siteList.Items[$siteIndex].Remove()
	$siteList.Refresh()
	
}

$siteOptions_Opened={ $siteOptions.Focus() }

$buttonSitesClose_Click={ $formSites.Close() }

$buttonAddSite_Click = { Call-site-add_psf; Reopen-Sites }

$buttonAddBrowser_Click = { Call-browser-add_psf; Reopen-Sites }

$addSiteToolStripMenuItem_Click = {
	
	if (((Get-ChildItem -Path "HKCU:\Software\Nexus\WebBrowsers").Count -lt 1) -eq $true)
	{
		$global:errorMessage = "There are currently no browsers added. Please add a browser before adding a new site."
		Call-error_psf
		Call-browser-add_psf
		$global:siteEditData = @()
		Reopen-Sites
	}
	else
	{
		Call-site-add_psf
		$global:siteEditData = @()
		Reopen-Sites
	}
	
}

$copyURLToolStripMenuItem_Click = { Set-Clipboard -Value "$script:site" }

$exportSitesToolStripMenuItem_Click={
	
	$websiteData = Get-ChildItem -Path "HKCU:\Software\Nexus\Websites" | Get-ItemProperty
	$count = $websiteData.Count
	$webArray = @()
	for ($index = 0; $index -lt $count; $index++)
	{
		$webObj = New-Object PSObject -Property @{
			
			AccountType = $websiteData[$index].AccountType
			Browser = $websiteData[$index].Browser
			Description = $websiteData[$index].Description
			Groupname = $websiteData[$index].Groupname
			URL = $websiteData[$index].URL
			PSPath = $websiteData[$index].PSPath.Split("\")[-1]
		}
		$webArray += $webObj
	}
	$saveFileDialog = New-Object System.Windows.Forms.SaveFileDialog
	$saveFileDialog.initialDirectory = $initialDirectory
	$saveFileDialog.filter = "CSV (*.csv)| *.csv"
	$saveFileDialog.ShowDialog() | Out-Null
	
	$saveLocation = $saveFileDialog.FileName
	$webArray | Select AccountType, Browser, Description, Groupname, URL, PSPath | Export-CSV -Path "$saveLocation" -NoTypeInformation
	
}

$importSitesToolStripMenuItem_Click={
	
	$OpenFileDialog = New-Object System.Windows.Forms.OpenFileDialog
	$OpenFileDialog.initialDirectory = $initialDirectory
	$OpenFileDialog.filter = "CSV (*.csv)| *.csv"
	$OpenFileDialog.ShowDialog() | Out-Null
	
	$sites = Import-CSV -Path $OpenFileDialog.Filename
	
	foreach ($site in $sites)
	{
		$path = "HKCU:\Software\Nexus\Websites\" + $site.PSPath
		New-Item "$path" -Force
		New-ItemProperty -Path "$path" -Name AccountType -PropertyType String -Value $site.AccountType -Force
		New-ItemProperty -Path "$path" -Name Browser -PropertyType String -Value $site.Browser -Force
		New-ItemProperty -Path "$path" -Name Description -PropertyType String -Value $site.Description -Force
		New-ItemProperty -Path "$path" -Name Groupname -PropertyType String -Value $site.Groupname -Force
		New-ItemProperty -Path "$path" -Name URL -PropertyType String -Value $site.URL -Force
	}
	
	$websiteGroups = @()
	$websiteGroups = $sites.Groupname | Select -Unique
	
	$path = "HKCU:\Software\Nexus\WebsiteGroups\"
	if ((Test-Path -Path $path) -eq $false)
	{ New-Item "$path" -Force }
	
	foreach ($group in $websiteGroups)
	{
		New-ItemProperty -Path "$path" -Name $group -PropertyType DWORD -Value 0 -Force
	}
	
}

$editSiteToolstripMenuItem_Click={
	
	$global:siteEditData = New-Object PSObject -Property @{
		
		SiteName = $siteList.Items[$siteIndex].SubItems[1].Text
		URL = $siteList.Items[$siteIndex].SubItems[2].Text
		Account = $siteList.Items[$siteIndex].SubItems[3].Text
		Group = $siteList.Items[$siteIndex].SubItems[4].Text
		RegPath = $siteList.Items[$siteIndex].SubItems[5].Text
		Browser = $siteList.Items[$siteIndex].SubItems[7].Text
		
	}
	
	if (((Get-ChildItem -Path "HKCU:\Software\Nexus\WebBrowsers").Count -lt 1) -eq $true)
	{
		$global:errorMessage = "There are currently no browsers added. Please add a browser before adding a new site."
		Call-error_psf
		Call-browser-add_psf
		$global:siteEditData = @()
		Reopen-Sites
	}
	else
	{
		Call-site-add_psf
		$global:siteEditData = @()
		Reopen-Sites
	}
	
}
